@Library('pipeline-library-demo1') _
pipeline {
  agent any
  tools {nodejs "node"}
    
  stages {     
    stage('git checkout') {
      steps {
        git 'https://github.com/anitha8242/AngularCalculator.git'
      }
    }
     
    stage('Build') {
      steps {
        sh 'npm install'
         sh 'npm install -g @angular/cli'
      }
    }  
    
            
    stage('Test') {
      steps {
        sh 'ng build'
      }
    }
  }
}
